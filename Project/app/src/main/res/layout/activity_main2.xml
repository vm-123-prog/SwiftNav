<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="283dp"
        android:layout_height="221dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.205"
        app:srcCompat="@mipmap/logol" />

    <Button
        android:id="@+id/geocodebtn"
        android:layout_width="245dp"
        android:layout_height="53dp"
        android:layout_marginTop="44dp"
        android:text="Geocoding"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/revgeocode"
        android:layout_width="245dp"
        android:layout_height="53dp"
        android:layout_marginTop="12dp"
        android:text="Reverse Geocoding"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/geocodebtn" />

    <Button
        android:id="@+id/businessbtn"
        android:layout_width="245dp"
        android:layout_height="53dp"
        android:layout_marginTop="12dp"
        android:text="Business Search"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/revgeocode" />
</androidx.constraintlayout.widget.ConstraintLayout>